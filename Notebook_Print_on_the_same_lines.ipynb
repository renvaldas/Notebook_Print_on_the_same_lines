   "source": [
    "import time\n",
    "from IPython.display import clear_output\n",
    "\n",
    "for f in range(15):\n",
    "    clear_output(wait=True)\n",
    "    print(\"Dynamically updated: %i\" % (f * 33))\n",
    "    print(f)\n",
    "    time.sleep(0.3)\n",
    "clear_output(wait=True)\n",
    "print(\"Done.\")"
   ]
  },
